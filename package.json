{
  "name": "enocean-core",
  "version": "0.8.2",
  "description": "A TypeScript implementation of the core functionality of the EnOcean protocol for Node.js.",
  "author": "Henning Kerstan <mail@henningkerstan.de>",
  "license": "Apache-2.0",
  "files": [
    "lib/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "pretest": "npm run lint",
    "test": "npx jasmine-ts --config=jasmine.json",
    "doc": "npx typedoc",
    "build": "npx tsc",
    "all": "npm run build && npm run build && npm run doc",
    "lint": "eslint --ignore-path .gitignore --fix src/**/*.ts spec/**/*.ts",
    "prepublishOnly": "npm run license-checker && npm run build",
    "version": "node .version.js",
    "prepare": "husky install",
    "license-checker": "npx license-checker --summary --production --unknown --onlyAllow \"Apache-2.0;MIT;BSD;ISC\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/henningkerstan/enocean-core.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@henningkerstan/byte": "^0.0.12",
    "@serialport/parser-inter-byte-timeout": "^9.0.7",
    "serialport": "^9.0.7"
  },
  "devDependencies": {
    "@types/jasmine": "^3.7.4",
    "@types/node": "^15.3.0",
    "@types/serialport": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "console-stamp": "^3.0.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.23.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.0",
    "jasmine": "^3.7.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.3",
    "license-checker": "^25.0.1",
    "np": "^7.5.0",
    "prettier": "^2.3.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  }
}
